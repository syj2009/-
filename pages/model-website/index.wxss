.contain {
  width: 100%;
  height: 100%;
}

.website-bg-img {
  width: 100%;
  background-size: 100%;
  background-repeat: repeat-y;
}

.banner-top {
  width: 100%;
}

.border-bg-img {
  margin-top: 40rpx;
  width: 80%;
  margin: 0 auto;
  background-repeat: no-repeat;
  background-size: 100%;
  background-color: #fff;
  border: 10rpx solid #105da3;
  border-radius: 20rpx;
  margin-bottom: 40rpx;
  padding-bottom: 50rpx;
  position: relative;
}

.img-box-center {
  text-align: center;
  position: relative;
  top: -70rpx;
}

.head-img {
  width: 140rpx;
  height: 140rpx;
  border-radius: 140rpx;
  box-shadow: 0 0 10px #169;
}

.head-title {
  height: 70rpx;
}

.organizationName{
  margin-top: -50rpx;
  text-align: center;
  font-size: 40rpx;
  color: #333;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.organizationContent {
  margin: 0 14rpx;
  background-color: #f2f3f8;
  font-size: 26rpx;
  color: #666;
  padding: 15rpx;
}

.head-title {
  width: 80%;
  margin: 60rpx auto;
  background-repeat: no-repeat;
  background-size: 100%;
  text-align: center;
  padding-top: 5rpx;
}

.institution-advantage-list {
  padding: 0 30rpx;
}

.institution-advantage-item {
  overflow: hidden;
  border-bottom: 1rpx solid #ddd;
  padding-bottom: 50rpx;
}

.float-left {
  float: left;
  margin-right: 20rpx;
}

.float-left:last-child {
   margin-right: 0rpx;
}

.institution-img {
  width: 180rpx;
  height: 108rpx;
}

.institution-descr {
  width: 60%;
  font-size: 26rpx;
  color: #666;
}

.left-gou {
  width: 30rpx;
  height: 150rpx;
  position: absolute;
  bottom: -110rpx;
  left: 30rpx;
  z-index: 20;
}

.right-gou {
  width: 30rpx;
  height: 150rpx;
  position: absolute;
  bottom: -110rpx;
  right: 30rpx;
  z-index: 20;
}

.project-list {
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
}

.project-item {
  width: 260rpx;
  height: 250rpx;
}

.project-img {
  width: 260rpx;
  height: 168rpx;
  border-radius: 10rpx;
  box-shadow: 0 0 10 #169;
}

.project-title {
  font-size: 34rpx;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}

.sign-up-view {
  width: 65%;
  margin: 0 auto;
  text-align: center;
  display: flex;
  flex-direction: column;
  align-content: flex-start;
}

.code-img {
  width: 310rpx;
  height: 310rpx;
}

.code-view {
  width: 370rpx;
  height: 340rpx;
  background-color: #fff;
  border-radius: 10rpx;
  text-align: center;
  padding-top: 30rpx;
  margin: 0 auto;
}

.code-descr {
  margin-top: 50rpx;
  color: #fff;
  font-size: 34rpx;
  margin-bottom: 70rpx;
}

.name-box {
  height: 70rpx;
  line-height: 70rpx;
  margin-top: 0rpx;
  margin-bottom: 34rpx;
  background-color: #fff;
  border-radius: 10rpx;
}

.submit {
  width: 360rpx;
  height: 70rpx;
  background-color: #fed449;
  border: 1rpx solid #000;
  font-size: 30rpx;
  color: #333;
  text-align: center;
  line-height: 70rpx;
  margin: 44rpx auto;
  border-radius: 10rpx;
}

.name-box input {
  height: 70rpx;
  line-height: 70rpx;
}