.contain {
  width: 100%;
  height: 100%;
}

.website-bg-img {
  width: 100%;
  background-size: 100%;
  background-repeat: repeat-y;
}


.head-view {
  text-align: center;
  padding-top: 120rpx;
}

.head-img {
  width: 200rpx;
  height: 350rpx;
}

/*描述图片  */
.class-describe-view {
  width: 642rpx;
  height: 374rpx;
  margin: 0 auto;
  border: 28rpx solid rgba(100, 207, 176, 0.6);
  margin-top: 80rpx;
}

.class-describe-img {
  width: 642rpx;
  height: 374rpx;
  border-radius: 10rpx;
}

/*介绍类的边框  */
.fit-describe-view {
  width: 602rpx;
  margin: 0 auto;
  border: 28rpx solid rgba(100, 207, 176, 0.6);
  margin-top: 80rpx;
  padding: 66rpx 20rpx 40rpx;
}

.head-title {
   width: 602rpx;
   background-repeat: no-repeat;
   background-size: 100%;
   text-align: center;
}

.input-box {
  color: #fff;
}

.content-describe {
  margin-top: 50rpx;
  margin-bottom: 120rpx;
  font-size: 28rpx;
  color: #666;
}

.fit-describe-img {
  width: 602rpx;
  height: 374rpx;
  border-radius: 10rpx;
}

/*班级课程图片的样式  */

.course-img-box {
  margin-top: 50rpx;
  position: relative;
}

.fit-describe-span {
  position: absolute;
  bottom: 50rpx;
  height: 92rpx;
  width: 590rpx;
  background-color: rgba(100, 207, 176, 0.8);
  font-size: 28rpx;
  color: #fff;
  padding: 10rpx;
  border: 1rpx solid #00f0ff;
}

.course-title {
  font-size: 34rpx;
  color: #000;
  margin-top: 30rpx;
}

.course-item {
  margin-bottom: 50rpx;
}

.course-item:last-child {
  margin-bottom: 0rpx;
}

/*底部报名页面  */
.school-info {
  text-align: center;
  margin-top: 120rpx;
}

.school-logo {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
}

.school-title {
  margin-top: 20rpx;
  margin-bottom: 50rpx;
}

.school-code {
  width: 200rpx;
  height: 200rpx;
}

.code-describe {
  margin-top: 20rpx;
}

.input-view {
  margin-top: 30rpx;
  width: 60%;
  height: 80rpx;
  line-height: 80rpx;
  border: 1rpx solid #ddd;
  border-radius: 10rpx;
  margin: 30rpx auto;
}

.input-name {
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  color: #000;
}

.signUp {
  width: 50%;
  height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  border-radius: 80rpx;
  background-color: rgb(100, 207, 176);
  line-height: 80rpx;
  margin: 60rpx auto;
  margin-bottom: 0;
}

.website-bg-img {
  padding-bottom: 50rpx;
}

/*描边样式  */
.border-blue-position {
  display: inline-block;
  padding: 20rpx;
  border: 1rpx solid #00f0ff;
  position: relative;
  top: -20rpx;
  right: 20rpx;
}

.border-blue {
  display: inline-block;
  padding: 20rpx;
  border: 1rpx solid #00f0ff;
  position: relative;
}

.text-border-blue {
  padding: 20rpx;
  border: 1rpx solid #00f0ff;
  position: relative;
}

.text-border-blue-title {
  padding: 20rpx;
  border: 1rpx solid #00f0ff;
  position: relative;
  top: -20rpx;
}

.edite-box {
  display: inline-block;
  width: 68rpx;
  height: 70rpx;
  background-color: rgba(0, 0, 0, 0.4);
  text-align: center;
  line-height: 80rpx;
  position: absolute;
  right: 0;
  top: -70rpx;
  border-radius: 10rpx;
}

.edite-icon {
  width: 40rpx;
  height: 40rpx;
}

/*模版信息  */
.hidden {
  display: none;
}

.bg-black {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.8);
  z-index: 999;
}

.alert-view {
  height: 410rpx;
  width: 610rpx;
  margin: auto;
  background-color: #fff;
  margin-top: 300rpx;
  border-radius: 10rpx;
  padding-top: 10rpx;
  overflow: hidden;
}

.text-area-box {
  width: 550rpx;
  height: 280rpx;
  border: 1rpx solid #ddd;
  padding: 10rpx;
  margin: 10rpx auto;
  position: relative;
  font-size: 28rpx;
  color: #999;
  line-height: 36rpx;
}

.text-area-number {
  position: absolute;
  bottom: 10rpx;
  right: 10rpx;
}

.btn-box {
  height: 110rpx;
  border-top: 1rpx solid #ddd;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.cancle-btn {
  width: 50%;
  text-align: center;
  font-size: 30rpx;
  color: #888;
  line-height: 90rpx;
  height: 90rpx;
}

.submit-btn {
  width: 50%;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  line-height: 90rpx;
  background-color: #e26353;
  height: 90rpx;
}

/*模版封面样式  */
.cover-view {
  background-color: #fff;
  height: 780rpx;
  width: 610rpx;
  border-radius: 10rpx;
  margin: 0 auto;
  margin-top: 100rpx;
  overflow: hidden;
}

.cover-title-view {
  height: 180rpx;
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 180rpx;
  border-bottom: 1rpx solid #ddd;
}

.cover-name-view{
  height: 88rpx;
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 88rpx;
  border-bottom: 1rpx solid #ddd;
}

.price-view {
  height: 100rpx;
  padding: 0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 100rpx;
  border-bottom: 1rpx solid #ddd;
}

.cover-desc-view {
  height: 302rpx;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #ddd;
}

.cover-title-name {
  font-size: 34rpx;
  color: #333;
}

.cover-title-name-red {
  font-size: 34rpx;
  color: #de644f;
}

.fit-margin {
  margin-top: 20rpx;
  margin-bottom: 15rpx;  
}

.cover-title-shcool {
  font-size: 34rpx;
  color: #666;
}

.cover-title-img {
  width: 200rpx;
  height: 120rpx;
  border-radius: 10rpx;
  margin-top: 30rpx;
}

.textarea-box {
  background-color: #f7f7f7;
  width: 570rpx;
  height: 220rpx;
  margin: 0 auto;
  font-size: 28rpx;
  color: #999;
  position: relative;
}

.textarea-style {
  padding: 15rpx;
}

/*底部保存按钮  */
.nav-bottom {
  height: 88rpx;
  line-height: 88rpx;
  color: #fff;
  display: flex;
  flex-direction: row;
  justify-content: center;
  background-color: #000;
}

.save-btn {
  height: 88rpx;
  font-size: 30rpx;
}