page{
  display: block;
  margin: 0 auto;
  min-height: 100%;
  background-color: #eee;
}
.contain{
  width: 710rpx;
  margin: auto;
  text-align: center;
}
.back-red{
  z-index: -1;
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  height: 190rpx;
  background-color: #EB6550;
}
/*占位*/
.sitting{
  height: 50rpx;
}

/* 正文 */
 .view-information{
  width: 100%;
  height: 200rpx;
  border-radius: 8rpx;
  background-color: #fff;
} 
.frequency{
  float: left;
  margin: 50rpx 0 auto;
  width: 49%;
  border-right: 1rpx solid #eaeaea;
}
.visit{
  float: left;
  margin: 50rpx 0 auto;
  width: 50%;
}
.prompt{
  font-size: 36rpx;
  color: #333;
  text-align: left;
  margin-bottom: 26rpx;
}
.frequency-number,.visit-number{
  font-size: 44rpx;
  color: #de644f;
}
.jinru{
  width: 14rpx;
  height: 24rpx;
  padding-bottom: 5rpx;
  margin-left: 20rpx;
}

/* 报名学员列表 */
.sign-up-title{
  color: #333;
  font-size: 40rpx;
  height: 88rpx;
  line-height: 88rpx;
  text-align: left;
  margin-left: 30rpx;
}
.sign-up-number{
  color: #de644f;
}
.sign-up-list{
  width: 710rpx;
  background-color: #fff;
}
.sign-up{
  height: 88rpx;
  line-height: 88rpx;
  margin:0 30rpx;
  text-align: left;
  border-bottom: 1rpx solid #eaeaea;
}
.view-jinru{
  width: 14rpx;
  height: 24rpx;
  margin-left: 16rpx;
}
.noBorderBottom{
  border-bottom: 0;
}
.prompt{
  font-size: 34rpx;
  color: #333;
}
.static{
  font-size: 34rpx;
  color: #999;
}
.view-static{
  float: right;
}